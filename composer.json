{
    "name": "rector/custom-phpspec-to-phpunit",
    "type": "rector-extension",
    "license": "MIT",
    "description": "Rector migration rules for PHPSpec to PHPUnit",
    "bin": [
        "bin/phpspec-to-phpunit",
        "bin/phpspec-to-phpunit.php"
    ],
    "require": {
        "php": ">=8.2",
        "symfony/finder": "^7.0",
        "tomasvotruba/phpunit-json-result-printer": "^0.1.1",
        "webmozart/assert": "^1.11",
        "nette/utils": "^4.0",
        "symfony/console": "^7.1"
    },
    "require-dev": {
        "rector/rector": "^2.0",
        "nikic/php-parser": "^5.4",
        "phpunit/phpunit": "^11.5",
        "phpstan/phpstan": "^2.1",
        "phpecs/phpecs": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "tomasvotruba/class-leak": "^2.0",
        "tracy/tracy": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Rector\\PhpSpecToPHPUnit\\": ["src", "rules"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\PhpSpecToPHPUnit\\Tests\\": ["tests", "rules-tests"]
        },
        "classmap": [
            "stubs",
            "tests/Sets/Source"
        ],
        "files": [
            "tests/functions.php"
        ]
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "phpunit":  "vendor/bin/phpunit --colors=always"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "rector/extension-installer": true
        }
    }
}
