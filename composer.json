{
    "name": "rector/custom-phpspec-to-phpunit",
    "type": "rector-extension",
    "license": "MIT",
    "description": "Rector migration rules for PHPSpec to PHPUnit",
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "rector/rector": "^0.18.5",
        "phpunit/phpunit": "^10.3",
        "phpstan/phpstan": "^1.10.36",
        "symplify/phpstan-rules": "^11.4",
        "symplify/phpstan-extensions": "^11.2",
        "symplify/easy-coding-standard": "^12.0",
        "symplify/rule-doc-generator": "^12.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-webmozart-assert": "^1.2.2",
        "symplify/easy-ci": "^11.3",
        "tomasvotruba/unused-public": "^0.3",
        "tomasvotruba/class-leak": "^0.1",
        "tracy/tracy": "^2.10"
    },
    "autoload": {
        "psr-4": {
            "Rector\\PhpSpecToPHPUnit\\": ["src", "rules"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rector\\PhpSpecToPHPUnit\\Tests\\": ["rules-tests"]
        },
        "classmap": [
            "stubs",
            "tests/Sets/Source"
        ],
        "files": [
            "tests/functions.php"
        ]
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify",
        "check-cs": "vendor/bin/ecs check --ansi",
        "fix-cs": "vendor/bin/ecs check --fix --ansi",
        "docs": "vendor/bin/rule-doc-generator generate src rules --output-file docs/rector_rules_overview.md --ansi"
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "rector/extension-installer": true,
            "cweagans/composer-patches": false
        }
    }
}
